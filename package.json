{
  "name": "@whatskit/s3deployer-cli",
  "version": "0.1.2",
  "homepage": "https://github.com/whatskit/s3deployer-cli",
  "bugs": "https://github.com/whatskit/s3deployer-cli/issues",
  "repository": "git@github.com:whatskit/s3deployer-cli",
  "author": "Billgo <hi@billgo.me>",
  "license": "UNLICENSED",
  "bin": "./dist/sh.js",
  "scripts": {
    "build": "yarn clean && npm version patch --no-git-tag-version && yarn run build:cli",
    "build:cli": "tsc -p ./tsconfig.json && chmod +x ./dist/sh.js",
    "clean": "node tools/cleanup",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "test:cli": "./dist/sh.js --pwd ./test --stage test",
    "test:ts-node:cli": "ts-node -r tsconfig-paths/register ./src/sh.ts --pwd ./test --stage test",
    "lint": "eslint --fix"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.193.0",
    "@aws-sdk/client-s3": "^3.193.0",
    "@aws-sdk/client-sts": "^3.193.0",
    "@aws-sdk/credential-provider-env": "^3.193.0",
    "@aws-sdk/credential-provider-ini": "^3.193.0",
    "@aws-sdk/node-config-provider": "^3.193.0",
    "@aws-sdk/util-endpoints": "^3.193.0",
    "@whatskit/node-stage": "^0.1.0",
    "chalk": "^4.1.2",
    "fast-glob": "^3.2.12",
    "micromatch": "^4.0.5",
    "mime-types": "^2.1.35",
    "prompt-sync": "^4.2.0",
    "semver": "^7.3.8",
    "simple-git": "^3.14.1",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.193.0",
    "@jest/types": "^29.2.1",
    "@swc/core": "^1.3.9",
    "@swc/jest": "^0.2.23",
    "@types/diff": "^5.0.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^29.2.0",
    "@types/micromatch": "^4.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.3",
    "@types/prettier": "^2.7.1",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  }
}
